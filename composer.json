{
    "name": "fiteco-group/kimai-client",
    "description": "Kimai V2 PHP Api client.",
    "keywords": [
        "openapitools",
        "openapi-generator",
        "openapi",
        "php",
        "sdk",
        "rest",
        "api"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.2|>=8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7",
        "guzzlehttp/psr7": "^1.7 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "friendsofphp/php-cs-fixer": "^2.12"
    },
    "autoload": {
        "psr-4": { "Fiteco\\KimaiClient\\" : "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Fiteco\\KimaiClient\\Test\\" : "test/" }
    }
}
